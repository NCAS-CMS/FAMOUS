      PARAMETER(                                                           ICLPRM3A.21    
     &     NPD_ICE_CLOUD_FIT=6                                             ADB2F404.1729  
     &   , IP_SLINGO_SCHRECKER_ICE=1                                       ICLPRM3A.23    
     &   , IP_ICE_UNPARAMETRIZED=3                                         ICLPRM3A.25    
     &   , IP_SUN_SHINE_VN2_VIS=4                                          ICLPRM3A.26    
     &   , IP_SUN_SHINE_VN2_IR=5                                           ICLPRM3A.27    
     &   , IP_ICE_ADT=6                                                    ADB2F404.1730  
     &   )                                                                 ICLPRM3A.28    
