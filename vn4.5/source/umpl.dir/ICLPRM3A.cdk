!                                                                          ICLPRM3A.4     
!     MODULE TO SET NUMBERS FOR ICE CLOUD SCHEMES.                         ICLPRM3A.5     
!                                                                          ICLPRM3A.6     
      INTEGER                                                              ICLPRM3A.7     
     &     NPD_ICE_CLOUD_FIT                                               ICLPRM3A.8     
!             NUMBER OF CLOUD FITTING SCHEMES                              ICLPRM3A.9     
     &   , IP_SLINGO_SCHRECKER_ICE                                         ICLPRM3A.10    
!             PARAMETRIZATION OF SLINGO AND SCHRECKER.                     ICLPRM3A.11    
     &   , IP_ICE_UNPARAMETRIZED                                           ICLPRM3A.14    
!             UNPARAMETRIZED ICE CRYSTAL DATA                              ICLPRM3A.15    
     &   , IP_SUN_SHINE_VN2_VIS                                            ICLPRM3A.16    
!             SUN AND SHINE'S PARAMETRIZATION IN THE VISIBLE (VERSION 2)   ICLPRM3A.17    
     &   , IP_SUN_SHINE_VN2_IR                                             ICLPRM3A.18    
!             SUN AND SHINE'S PARAMETRIZATION IN THE IR (VERSION 2)        ICLPRM3A.19    
